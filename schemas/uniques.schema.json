{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Uniques",
    "type": "array",
    "description": "A list of uniques to apply to the object. See https://yairm210.github.io/Unciv/Modders/uniques/",
    "items": {
        "type": "string",
        "anyOf": [
            {
                "type": "string",
                "pattern": "^[^><]*(\\<[^>]*\\>[^><]*)*$"
            },
            {
                "type": "string",
                "pattern": "^[^\\[\\]]*(\\[[^\\[\\]]*\\][^\\[\\]]*)*$"
            }
        ]
    }
}
