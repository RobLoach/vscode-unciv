{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
      "type": "object",
      "properties": {
          "name": { "type": "string" },
          "movementType": {
              "enum": ["Land", "Water", "Air"]
          },
          "uniques": { "$ref": "uniques.schema.json" },
          "civilopediaText": { "$ref": "civilopediaText.schema.json" }
      },
      "required": [
          "name", "movementType"
      ],
      "additionalProperties": false
  }
}
