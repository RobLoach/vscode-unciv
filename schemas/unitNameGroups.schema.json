{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "The name of the unit name group. For example: Scientist."
      },
      "unitNames": {
        "type": "array",
        "description": "A list of names that apply to the group. For example: Alan Turing.",
        "items": { "type": "string" }
      },
      "uniques": { "$ref": "uniques.schema.json" },
      "civilopediaText": { "$ref": "civilopediaText.schema.json" }
    },
    "required": [
      "name"
    ],
    "additionalProperties": false
  }
}
